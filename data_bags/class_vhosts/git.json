{
  "id": "git",
  "server_roles": ["class_appserver_dev"],
  "vhosts": {
    "*:80": {
      "ServerName": "git.minions.umn.edu",
      "ServerAlias": "git.class.umn.edu",
      "RedirectMatch": {
        "/(.*)": "https://git.minions.umn.edu/$1"
      }
    },
    "*:443": {
      "ServerName": "git.minions.umn.edu",
      "ServerAlias": "git.class.umn.edu",
      "DocumentRoot": "/data/www/macro/",
      "SSLCertificate": "git_class_umn_edu",
      "SSLCertificateChain": "incommon",
      "Directory": {
        "/data/www/macro": {
          "AllowOverride": "All"
        }
      }
    }
  }
}
