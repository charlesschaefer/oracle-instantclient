{
  "id": "class",
  "server_roles": ["class_appserver_master","class_appserver_slave"],
  "vhosts": {
    "*:10080": {
      "ServerName": "www.class.umn.edu",
      "ServerAlias": "class.umn.edu",
      "DocumentRoot": "/data/www/crimson/",
      "CommID": 10,
      "Redirect": {
        "/alumni/": "https://www.class.umn.edu/alumni/",
        "/docs/": "https://www.class.umn.edu/docs/",
        "/home.php": "https://www.class.umn.edu/crimsonAdmin/home.php",
        "/mentor/": "https://www.class.umn.edu/mentor/"
      },
      "Directory": {
        "/data/www/htdocs/crimson/keynote": {
          "ErrorDocument": "404 /crimson/keynote/dead_screen.png"
        }
      }
    },
    "*:10443": {
      "ServerName": "www.class.umn.edu",
      "ServerAlias": "class.umn.edu",
      "SSLCertificate": "class_umn_edu",
      "SSLCertificateChain": "incommon",
      "DocumentRoot": "/data/www/crimson/",
      "CommID": 10,
      "Redirect": {
        "/home.php": "https://www.class.umn.edu/crimsonAdmin/home.php"
      },
      "Alias": {
        "/alumni/": "/data/www/alumni/",
        "/docs/": "/data/www/docs/",
        "/mentor/": "/data/www/alumni/",
        "/secure/": "/data/www/secure/",
        "/test/": "/data/www/test/",
        "/test_two/": "/data/www/test_two/"
      },
      "Location": {
        "/secure/": {
          "DirectoryIndex": "index.html index.php"
        }
      },
      "Directory": {
        "/data/www/secure/cla_open_house": {
          "AllowOverride": "all"
        },
        "/data/www/secure/code": {
          "AllowOverride": "all"
        },
        "/data/www/htdocs/crimson/keynote": {
          "ErrorDocument": "404 /crimson/keynote/dead_screen.png"
        }
      }
    }
  }
}
