{
  "id": "auth",
  "server_roles": ["class_appserver_master","class_appserver_slave"],
  "vhosts": {
    "*:12080": {
      "ServerName": "auth.class.umn.edu",
      "RedirectMatch": {
        "/(.*)": "https://auth.class.umn.edu/$1"
      }
    },
    "*:12443": {
      "ServerName": "auth.class.umn.edu",
      "DocumentRoot": "/data/www/minion_auth/",
      "SSLCertificate": "auth_class_umn_edu"
    }
  }
}
